{
  "title": "status of plc",
  "definitions": {
    "rt": "wawesome.plc.status",
    "type": "object",
    "properties": [
      {
        "name": "name",
        "type": "string",
        "description": "name of runtime"
      },
      {
        "name": "runtime",
        "type": "string",
        "description": "runtime software name"
      },
      {
        "name": "runtime-ver",
        "type": "string",
        "description": "version of runtime software"
      },
      {
        "name": "plc-app",
        "type": "string",
        "description": "managed app software name"
      },
      {
        "name": "plc-app-ver",
        "type": "string",
        "description": "version of managed app"
      },
      {
        "name": "plc-status",
        "type": "string",
        "description": "available value: running , ready , debugging"
      },
      {
        "name": "code",
        "type": "integer",
        "description": "code"
      },
      {
        "name": "mgt-status",
        "type": "string",
        "description": "manage state"
      },
      {
        "name": "mgt-error",
        "type": "string",
        "description": "manageerror"
      },
      {
        "name": "swap-app-ready",
        "type": "boolean",
        "description": "true if ready to swap app"
      },
      {
        "name": "mgt-status-enter-time",
        "type": "integer",
        "description": "manage state enter time in ms from 1970.1.1"
      }
    ]
  },
  "type": "object",
  "required": [  ]
}
